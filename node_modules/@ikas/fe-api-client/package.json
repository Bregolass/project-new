{
  "name": "@ikas/fe-api-client",
  "version": "1.0.19",
  "main": "./build-cjs/index.js",
  "module": "./build/index.js",
  "author": "Umut Ozan Yıldırım",
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/build",
    "/build-cjs"
  ],
  "exports": {
    ".": {
      "require": "./build-cjs/index.js",
      "import": "./build/index.js"
    }
  },
  "bin": {
    "api-generator": "./build/api-generator.js"
  },
  "scripts": {
    "build": "rm -rf ./build && rollup -c",
    "prepack": "npm run build",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest",
    "generate-graph-models": "ts-node ./src/scripts/index.ts"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.4.1",
    "@graphql-tools/url-loader": "^7.5.2",
    "@redtea/format-axios-error": "^2.1.0",
    "command-line-args": "^5.2.0",
    "graphql": "^15.3.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/command-line-args": "^5.2.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/testing-library__jest-dom": "^5.9.3",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axios": "^0.26.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.2",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "rollup": "^2.38.4",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-mjs-entry": "^0.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-rename-node-modules": "^1.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^26.4.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "typescript-transform-paths": "^2.2.2",
    "typings": "^2.1.1"
  },
  "peerDependencies": {
    "axios": "^0.26.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    },
    "ignorePatterns": "build",
    "rules": {
      "getter-return": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-empty-interface": "off",
      "@typescript-eslint/triple-slash-reference": "off",
      "@typescript-eslint/interface-name-prefix": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-useless-constructor": "off"
    }
  }
}
